{
  "name": "@razz21/vue-scan",
  "version": "0.1.1",
  "type": "module",
  "author": "Razz21",
  "license": "MIT",
  "description": "Track and visualize Vue 3 component renders",
  "keywords": [
    "vue",
    "vue3",
    "vue-scan",
    "development",
    "performance",
    "debug",
    "debugging",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Razz21/vue-scan.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.min.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.min.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "devDependencies": {
    "vite-plugin-dts": "^4.5.3",
    "vue": "*"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  }
}
